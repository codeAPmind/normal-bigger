float viewport_inv_width;
float viewport_inv_height;
sampler colormap;

const float4 samples[9] = {
-1.0, -1.0, 0, 1.0/16.0,
-1.0, 1.0, 0, 1.0/16.0,
1.0, -1.0, 0, 1.0/16.0,
1.0, 1.0, 0, 1.0/16.0,
-1.0, 0.0, 0, 2.0/16.0,
1.0, 0.0, 0, 2.0/16.0,
0.0, -1.0, 0, 2.0/16.0,
0.0, 1.0, 0, 2.0/16.0,
0.0, 0.0, 0, 4.0/16.0
};
float4 ps_main(float2 texcoord: TEXCOORD0) : COLOR
{
   float4 col = float4(0,0,0,0);
   // Sample and output the averaged colors
    for(int i=0;i<9;i++)
     col += samples[i].w * tex2D(colormap,texcoord+
        float2(samples[i].x * 0.5,samples[i].y * 0.8));
  return col;
  
   //return tex2D(Texture0Map,texcoord);
}
