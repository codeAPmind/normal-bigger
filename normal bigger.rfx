    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
//float4 lightDir;
sampler colormap;
sampler toonmap;
float4 ps_main(float Position:POSITION,float2 Texcoord:TEXCOORD0, float3 normal:TEXCOORD1,float2 Texcoord1:TEXCOORD2):COLOR
{
   //sample the filtered environment map
   float4 color = tex2D(colormap,Texcoord);
   float toon = tex2D(toonmap, Texcoord1);
   color = (0.7 + 0.7 * toon) * color;
   color.a = 1.0f;
   return color;
}]]></RmShaderCode>
